	"background_page": "background.html",
	"homepage_url": "http://www.taskforceapp.com/api",
	"icons": {
		"128": "icon128.png",
		"48": "icon48.png"
	},
	"options_page" : "options.html",
	"permissions":
		["tabs", "https://taskforceapp.com/", "https://www.taskforceapp.com/"],
	"browser_action": {
		"default_icon": "icon.png",
		"default_title": "Send to Taskforce"
	}
}